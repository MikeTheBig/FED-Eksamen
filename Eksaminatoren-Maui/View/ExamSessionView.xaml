<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    x:Class="Eksaminatoren_Maui.Views.ExamSessionView"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Eksamenssession">
    <VerticalStackLayout Padding="20" Spacing="15">
        <Label Text="Eksamenssession" FontSize="24" HorizontalOptions="Center"/>
        <Label Text="{Binding Student.StudentNumber, StringFormat='Studienummer: {0}'}" FontSize="18"/>
        <Label Text="{Binding Student.Name, StringFormat='Navn: {0}'}" FontSize="18"/>
    </VerticalStackLayout>
</ContentPage>